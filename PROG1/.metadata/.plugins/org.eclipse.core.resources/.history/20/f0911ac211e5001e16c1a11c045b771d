#include "led.h"
#include "gpio.h"

void led_init(void);
void led_set_state(led_s_t led)
{
	if (led.state == LED_ON)
		{
			switch (led.number)
			{
			case LED_1: // Убраны лишние символы точек с запятой после case и использованы константы без скобок
				{
					// Код для установки состояния для LED_1 при включении
					break; // Пропущен break для завершения выполнения case
				}
			case LED_2: // Включены случаи для LED_2 и других LED (если применимо)
				{
					// Код для установки состояния для LED_2 при включении
					break; // Пропущен break для завершения выполнения case
				}
			// Другие случаи для других LED, если применимо
			}
		}
		// Другие условия и действия при выключении LED, если требуется
	}
