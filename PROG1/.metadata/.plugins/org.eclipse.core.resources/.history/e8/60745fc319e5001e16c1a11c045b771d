#include "button.h"
#include "stm32f0xx_it.h"

button_state_e_t button_state = UNPRESSED;

void button_init(void)
{
	it_set_callback(button_cb);
}

button_state_e_t button_get_state(void)
{
	return 0;
}

void button_cb(void)
{
	button_state = PRESSED;
}
